{
	"name": "spectre",
	"version": "0.5.0",
	"description": "A multi-purpose bot for Discord.",
	"main": "dist/src/Spectre.js",
	"scripts": {
		"start": "cross-env DEV=\"true\" ts-node --files src/Spectre",
		"build": "rimraf dist && tsc",
		"lint": "eslint . --ext .ts",
		"pm2:delete": "pm2 delete Spectre",
		"pm2:logs": "pm2 logs Spectre --lines=200",
		"pm2:restart": "pm2 restart Spectre",
		"pm2:start-harmony": "pm2 start dist/src/Spectre.js --node-args=\"--harmony\" --name=\"Spectre\"",
		"pm2:start": "pm2 start dist/src/Spectre.js --name=\"Spectre\"",
		"pm2:stop": "pm2 stop Spectre"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jo3-l/spectre.git"
	},
	"keywords": [
		"discord",
		"spectre",
		"discord-bot"
	],
	"author": "jo3-l",
	"license": "MIT",
	"private": true,
	"dependencies": {
		"@vitalets/google-translate-api": "^3.0.0",
		"canvas": "^2.6.1",
		"canvas-constructor": "^3.1.0",
		"chalk": "^3.0.0",
		"cheerio": "^1.0.0-rc.3",
		"common-tags": "^1.8.0",
		"d3-format": "^1.4.2",
		"discord-akairo": "github:discord-akairo/discord-akairo",
		"discord.js": "github:discordjs/discord.js",
		"fs-nextra": "^0.4.7",
		"mathjs": "^6.2.5",
		"module-alias": "^2.2.2",
		"moment": "^2.24.0",
		"moment-duration-format": "^2.3.2",
		"moment-timezone": "^0.5.27",
		"node-fetch": "^2.6.0",
		"pg": "^7.14.0",
		"pg-hstore": "^2.3.3",
		"reflect-metadata": "^0.1.13",
		"typeorm": "0.2.22",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"@types/cheerio": "^0.22.15",
		"@types/common-tags": "^1.8.0",
		"@types/d3-format": "^1.3.1",
		"@types/mathjs": "^6.0.2",
		"@types/module-alias": "^2.0.0",
		"@types/moment-duration-format": "^2.2.2",
		"@types/moment-timezone": "^0.5.12",
		"@types/ms": "^0.7.31",
		"@types/node-fetch": "^2.5.4",
		"@types/ws": "^6.0.4",
		"@typescript-eslint/eslint-plugin": "^2.9.0",
		"@typescript-eslint/parser": "^2.9.0",
		"cross-env": "^6.0.3",
		"eslint": "^6.7.2",
		"eslint-config-marine": "^5.3.2",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^3.7.2"
	},
	"_moduleAliases": {
		"@util": "src/util",
		"@structures": "src/structures",
		"@root": "."
	}
}

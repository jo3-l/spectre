{
	"extends": "marine",
	"rules": {
		"no-useless-constructor": "off",
		"@typescript-eslint/no-useless-constructor": "error",
		"linebreak-style": "off",
		"comma-dangle": [
			"error",
			"always-multiline"
		],
		"comma-spacing": "error",
		"comma-style": "error",
		"eol-last": "off",
		"max-len": [
			"error",
			{
				"code": 130
			}
		]
	},
	"parserOptions": {
		"createDefaultProgram": true
	}
}